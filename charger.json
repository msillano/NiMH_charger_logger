    {
        "id": "d8b1bc27.60a0b",
        "type": "function",
        "z": "a6941e78.e5a6b",
        "name": "reset start time, error",
        "func": "var tmp = Date.now();\nflow.set(\"starttime\", tmp);\nmsg.payload = tmp;\nflow.set(\"csec\",0);\nflow.set(\"cmin\",0);\nflow.set(\"error\",0);\n",
        "outputs": 0,
